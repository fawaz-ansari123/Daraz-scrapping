            var Group_umid = ctl.noConflict();
            Group_umid.config();
        } catch (err) {
            console.log(err);
        }
    
window.g_config = window.g_config || {};window.g_config.loadedCss = window.g_config.loadedCss || [];window.g_config.loadedCss = ["@ali/lzdmod-desktop-footer-daraz/pc/index.css"];
